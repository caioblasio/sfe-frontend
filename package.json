{
  "name": "sfe-frontend",
  "version": "1.0.0",
  "description": "Frontend Application for SFE mathematical Modeling Tool",
  "main": "index.js",
  "scripts": {
    "clean": "npm run clean:build",
    "clean:build": "npx rimraf ./dist",
    "prebuild": "npm run clean",
    "build": "npx webpack --config ./internals/webpack/webpack.build.js",
    "build:prod": "npx cross-env NODE_ENV=production npm run build",
    "build:dev": "npx cross-env NODE_ENV=dev npm run build -- --colors --progress",
    "start": "npx cross-env NODE_ENV=development webpack-dev-server --config ./internals/webpack/webpack.serve.js --colors --progress",
    "test": "npx cross-env NODE_ENV=test npx jest --coverage --verbose",
    "test:watch": "npx cross-env NODE_ENV=test npx jest --watchAll",
    "predeploy": "npm run build:prod",
    "deploy": "aws s3 sync dist/ s3://www.lapeamathmodels.xyz --acl public-read"
  },
  "keywords": [],
  "author": "Caio De Blasio",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^10.0.1",
    "babel-loader": "^8.0.6",
    "cross-env": "^7.0.2",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.3",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.5",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "csvtojson": "^2.0.10",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.5.3",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  }
}
